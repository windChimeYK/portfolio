{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "WhyK Portfolio Site",
  "author": "WhyK",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:css": "stylelint **/*.vue",
    "lintfix:css": "stylelint --fix **/*.vue",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
