{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "whyk portfolio site",
  "author": "WhyK",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:css": "stylelint **/*.vue",
    "lintfix:css": "stylelint --fix **/*.vue",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix:js": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "lint": "run-p lint:*",
    "lintfix": "run-p lintfix:*"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.2",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.6.0",
    "lint-staged": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "stylelint": "^10.1.0",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0"
  }
}
